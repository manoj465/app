{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "--------------dev commands--------------": "",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "--------------bde--------------": "",
    "start:bde": "expo start",
    "build:ios:bde:release": "expo build:ios --release-channel release",
    "build:android:bde:release": "expo build:android --release-channel release",
    "build:ios:bde:release::noPublish": "expo build:ios --release-channel release --no-publish",
    "publish:bde:beta": "expo publish --release-channel beta",
    "publish:bde:release": "expo publish --release-channel release",
    "--------------huelite--------------": "",
    "build:ios:huelite": "expo build:ios --release-channel release",
    "build:ios:huelite::noPublish": "expo build:ios --release-channel release --no-publish",
    "build:android:huelite": "expo build:android --release-channel release",
    "build:android:huelite::noPublish": "expo build:android --release-channel release --no-publish",
    "build:android:huelite:beta": "expo build:android --release-channel beta",
    "publish:huelite:beta": "expo publish --release-channel beta",
    "publish:huelite:release": "expo publish --release-channel release",
    "--------------web--------------": "",
    "web_comms": "",
    "web": "expo start --web",
    "deploy_web": "expo build:web && gh-pages -d web-build",
    "deploy_web_noBuild": "gh-pages -d web-build",
    "--------------git shortcuts--------------": "",
    "ADD_UNIVERSALS": "git subtree add --prefix=sternet/helpers/universals https://github.com/Abhimanyu6676/types-huelite.git main --squash",
    "PUSH_UNIVERSALS": "git subtree push --prefix=sternet/helpers/universals https://github.com/Abhimanyu6676/types-huelite.git main --squash",
    "PULL_UNIVERSALS": "git subtree pull --prefix=sternet/helpers/universals https://github.com/Abhimanyu6676/types-huelite.git main --squash",
    "ADD_LOGGER": "git subtree add --prefix=src/@logger https://github.com/Abhimanyu6676/-logger main --squash",
    "PUSH_LOGGER": "git subtree push --prefix=src/@logger https://github.com/Abhimanyu6676/-logger main --squash",
    "PULL_LOGGER": "git subtree push --prefix=src/@logger https://github.com/Abhimanyu6676/-logger main --squash"
  },
